package carproject.demo.dao;

create table car(	id BINARY(16) not null,	VIN VARCHAR(5) null,	make VARCHAR(15) null,	model VARCHAR(25) null,	year INT null,	mileage INT null,	price DOUBLE null,	constraint car_pk		primary key (id));
